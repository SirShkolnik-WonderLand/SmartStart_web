{
    "name": "smartstart-backend",
    "version": "1.0.0",
    "description": "SmartStart Backend API Server",
    "main": "server/consolidated-server.js",
    "scripts": {
        "start": "node server/consolidated-server.js",
        "start:api": "node server/consolidated-server.js",
        "dev": "node server/consolidated-server.js",
        "build": "cd frontend && npm install && npm run build",
