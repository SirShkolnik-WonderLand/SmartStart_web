        "dotenv": "^17.2.2",
        "express": "^4.21.1",
        "express-rate-limit": "^7.5.1",
        "helmet": "^8.1.0",
        "jsonwebtoken": "^9.0.2",
        "multer": "^2.0.2",
        "node-fetch": "^3.3.2",
        "nodemailer": "^7.0.6",
        "qrcode": "^1.5.4",
        "speakeasy": "^2.0.0",
        "zod": "^4.1.5"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/node": "^20",
        "eslint": "^9",
        "prisma": "^6.15.0",
