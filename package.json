{
    "name": "smartstart-backend",
    "version": "1.0.1",
    "description": "SmartStart Backend API Server",
    "main": "server/consolidated-server.js",
    "scripts": {
        "start": "node server/consolidated-server.js",
        "start:api": "node server/consolidated-server.js",
        "dev": "node server/consolidated-server.js",
        "build": "cd frontend && npm install && npm run build",
        "db:generate": "prisma generate",
        "db:push": "prisma db push",
        "db:migrate": "prisma migrate dev",
        "db:seed": "node prisma/seed.js",
        "lint": "eslint server/",
        "test": "jest",
        "test:unit": "jest tests/unit",
        "test:integration": "jest tests/integration",
        "test:api": "jest tests/api",
        "test:database": "jest tests/database",
        "test:security": "jest tests/security",
        "test:performance": "jest tests/performance",
        "test:venture-management": "jest tests/venture-management",
        "test:coverage": "jest --coverage",
        "test:watch": "jest --watch",
        "test:ci": "jest --ci --coverage --watchAll=false",
        "test:all": "jest --runInBand --detectOpenHandles"
    },
    "dependencies": {
        "@prisma/client": "^6.15.0",
        "bcryptjs": "^2.4.3",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dotenv": "^17.2.2",
        "express": "^4.21.1",
        "express-rate-limit": "^7.5.1",
        "helmet": "^8.1.0",
        "http-proxy-middleware": "^3.0.5",
        "jsonwebtoken": "^9.0.2",
        "multer": "^2.0.2",
        "node-fetch": "^3.3.2",
        "nodemailer": "^7.0.6",
        "qrcode": "^1.5.4",
        "speakeasy": "^2.0.0",
        "ws": "^8.14.2",
        "xstate": "^5.21.0",
        "zod": "^4.1.5"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/node": "^20",
        "@types/supertest": "^6.0.2",
        "autocannon": "^7.15.0",
        "babel-jest": "^29.7.0",
        "eslint": "^9",
        "jest": "^29.7.0",
        "jest-junit": "^16.0.0",
        "jest-sonar-reporter": "^2.0.0",
        "prisma": "^6.15.0",
        "supertest": "^7.0.0",
        "typescript": "^5"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "keywords": [
        "api",
        "backend",
        "express",
        "prisma",
        "postgresql",
        "jwt",
        "authentication"
    ],
    "author": "AliceSolutions",
    "license": "MIT"
}
