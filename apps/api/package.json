        "@types/bcryptjs": "^2.4.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/express-session": "^1.18.0",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/node": "^20.14.10",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "express": "^4.19.2",
        "express-rate-limit": "^7.5.1",
        "express-session": "^1.18.0",
        "helmet": "^7.2.0",
        "jsonwebtoken": "^9.0.2",
        "typescript": "^5.5.4",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/supertest": "^6.0.2",
